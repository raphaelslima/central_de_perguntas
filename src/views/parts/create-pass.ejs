<section>
  <h2>Crie sua própria sala</h2>
  <form action="/create-room" method="POST">
    <label for="room-pass" class="sr-only">Insira uma senha</label>
    <input
      type="password"
      name="password"
      id="room-pass"
      placeholder="Insira uma senha"
    />
    <button>
      <img src="/assets/images/user.svg" alt="Botão para criar sala" />
      Criar sala
    </button>
  </form>
</section>
